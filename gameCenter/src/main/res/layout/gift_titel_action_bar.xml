<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/actionbar_height_gift"
    android:background="@color/theme_green">

    <ImageButton
        android:id="@+id/not_serach_btn_back"
        android:layout_width="@dimen/not_serache_actionbar_btBack_width"
        android:layout_height="@dimen/not_serache_actionbar_btBack_height"
        android:layout_centerVertical="true"
        android:background="@drawable/button_background_selector"
        android:src="@mipmap/ic_back" />

    <TextView
        android:id="@+id/not_serach_tv_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_toRightOf="@+id/not_serach_btn_back"
        android:maxLines="1"
        android:textColor="@color/white"
        android:textSize="17sp" />


    <ImageButton
        android:id="@+id/ib_go_home"
        android:layout_width="@dimen/action_bar_height"
        android:layout_height="@dimen/action_bar_height"
        android:scaleType="center"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:paddingLeft="12dp"
        android:paddingRight="12dp"
        android:background="@drawable/button_background_selector"
        android:src="@mipmap/icon_backhome" />
</RelativeLayout>